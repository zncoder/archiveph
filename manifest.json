{
  "name": "archiveph",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Show archive.ph page",
  "permissions": [
    "tabs",
    "https://archive.ph/*"
  ],
  "icons": {
    "48": "icons/48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/32.png",
    "default_title": "Show archive.ph page"
  }
}
